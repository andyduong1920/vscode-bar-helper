{
  "name": "bar-helper",
  "displayName": "Bar Helper",
  "description": "Run any command from the status bar.",
  "version": "0.1.0",
  "publisher": "andyduong1920",
  "repository": {
    "type": "git",
    "url": "https://github.com/andyduong1920/vscode-bar-helper"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "Bar Helper",
      "properties": {}
    },
    "menus": {
      "editor/context": [
        {
          "command": "barHelper.runTestLine",
          "when": "editorTextFocus",
          "group": "navigation"
        },
        {
          "command": "barHelper.runTestFile",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "barHelper.runTestFile",
          "when": "explorerResourceIsFolder == false",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "key": "ctrl+z",
        "command": "barHelper.runTestLine",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+a",
        "command": "barHelper.runTestFile",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "barHelper.runTestFile",
        "title": "üöÄ Run the current test FILE üöÄ"
      },
      {
        "command": "barHelper.runTestLine",
        "title": "1Ô∏è‚É£ Run the current test LINE 1Ô∏è‚É£"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^18.15.11",
    "@types/vscode": "^1.77.0",
    "glob": "^9.3.4",
    "typescript": "^5.0.3"
  },
  "dependencies": {},
  "keywords": [
    "bar",
    "helper",
    "productive"
  ],
  "galleryBanner": {
    "color": "#0B162D",
    "theme": "dark"
  },
  "icon": "icon.png"
}
